{
    "files": [],
    "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
    "include": ["src"],
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "types": ["vite/client"],
        "paths": {
            "@/*": ["src/*"],
            "@data/*": ["src/data/*"],
            "@context/*": ["src/context/*"],
            "@layouts/*": ["src/layouts/*"],
            "@layouts": ["src/layouts/index.ts"],
            "@pages/*": ["src/pages/*"],
            "@pages": ["pages/index.ts"],
            "@components/*": ["src/components/*"],
            "@elements/*": ["src/elements/*"],
            "@assets/*": ["src/assets/*"]
        }
    }
}
